{
    "name": "slimtest/slimtest",
    "type": "project",
    "require": {
      "php": "^8.1",
        "slim/slim": "4.*",
        "slim/psr7": "^1.7",
        "php-di/php-di": "^7.0",
        "ext-json": "*",
        "symfony/console": "^6.4",
        "ramsey/uuid": "^4.9",
        "webmozart/assert": "^1.11",
        "guzzlehttp/guzzle": "^7.0",
        "doctrine/orm": "^3.5",
        "laminas/laminas-config-aggregator": "^1.18",
        "doctrine/migrations": "^3.9",
        "symfony/mailer": "^6.4",
        "twig/twig": "^3.21",
        "yoomoney/yookassa-sdk-php": "^3.10",
        "symfony/yaml": "^6.4"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Test\\": "tests/"
      }
    },
    "scripts" : {
      "app": "php bin/app.php --ansi ",
      "lint": "phplint",
      "test": "phpunit --display-warnings"
    },
    "require-dev": {
        "overtrue/phplint": "^9.6",
        "phpunit/phpunit": "^10.5"
    }
}
